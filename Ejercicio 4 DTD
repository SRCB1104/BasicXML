<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE artistas [
  <!ELEMENT artistas (artista+)>
  <!ELEMENT artista (nombre, discos)>
  <!ELEMENT nombre (#PCDATA)>
  <!ELEMENT discos (disco+)>
  <!ELEMENT disco (titulo, pais, compania, precio, ano-de-publicacion)>
  <!ELEMENT titulo (#PCDATA)>
  <!ELEMENT pais (#PCDATA)>
  <!ELEMENT compania (#PCDATA)>
  <!ELEMENT precio (#PCDATA)>
  <!ELEMENT ano-de-publicacion (#PCDATA)>
]>
